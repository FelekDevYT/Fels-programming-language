<filetype binary="false" default_extension="fels" description="Fels files" name="Fels">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="0x" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="remFunc;perror;panic;getVarValue;IsVarExists;remVar;defineVar;break;case;class;continue;do;else;extract;for;func;getNameFromNumber;if;match;new;print;println;return;toNumber;toString;typeOf;while" ignore_case="false" />
    <keywords2 keywords="NULL;array;echo;try" />
    <keywords3 keywords="false;null;this;true" />
    <keywords4 keywords="import;using" />
  </highlighting>
  <extensionMap>
    <mapping ext="fels" />
  </extensionMap>
</filetype>